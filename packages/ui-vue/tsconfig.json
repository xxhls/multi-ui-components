{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "strictNullChecks": false,
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "uni-starter-vue": ["src"],
      "uni-starter-vue/*": ["src/*", "*"]
    }
  },
  "include": ["components/**/*.ts", "components/**/*.tsx", "components/**/*.vue"],
  "exclude": ["node_modules"]
}
